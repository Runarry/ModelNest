{
  "name": "model-nest",
  "version": "1.1.0",
  "description": "跨平台 AIGC 模型管理与浏览工具，支持本地与 WebDAV 多数据源。",
  "main": "main.js",
  "scripts": {
    "start": "cross-env LOG_LEVEL=debug IS_DEV_MODE=true electron .",
    "pack": "cross-env IS_DEV_MODE=false LOG_LEVEL=warn electron-builder --dir -c electron-builder.yml",
    "dist": "cross-env IS_DEV_MODE=false LOG_LEVEL=warn electron-builder -c electron-builder.yml"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "electron-log": "^5.3.4",
    "electron-updater": "^6.6.2",
    "sharp": "^0.34.1",
    "webdav": "^5.8.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^36.0.0",
    "electron-builder": "^26.0.13"
  },
  "author": "Banazzle",
  "license": "MIT"
}
